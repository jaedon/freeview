<?xml version="1.0" ?>
<!-- ref. http://dbus.freedesktop.org/doc/dbus-specification.html -->
<node name="/Octopus/Appkit/Cas/Mmi">   
    <interface name="Octopus.Appkit.Cas.Mmi">

    <method name="GetEvtInfo">
    	<arg type="(uuuuus)" name="eventInfo" direction="out" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>
  
    <method name="GetUiEvent">
        <arg type="u" name="sessionType" direction="in" />
        <arg type="u" name="handleSession" direction="in" />
        <arg type="(uuuuus)" name="eventInfo" direction="out" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>                                                                        
  
    <method name="SendUiResponse">
        <arg type="u" name="groupId" direction="in" />
        <arg type="u" name="slot" direction="in" />
        <arg type="u" name="sessionType" direction="in" />
        <arg type="u" name="handleSession" direction="in" />
        <arg type="ay" name="inputData" direction="in" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>      
    
    <method name="CloseSession">
        <arg type="u" name="groupId" direction="in" />
        <arg type="u" name="slot" direction="in" />
        <arg type="u" name="sessionType" direction="in" />
        <arg type="u" name="handleSession" direction="in" />
        <annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>    
    
     <method name="OpenSession">
        <arg type="u" name="groupId" direction="in" />
        <arg type="u" name="slot" direction="in" />
        <arg type="u" name="sessionType" direction="in" />
        <arg type="ay" name="paramData" direction="in" />
        <arg type="u" name="handleSession" direction="out" />
        <annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>     
     
    <signal name="event">
      <arg type="u" name="param1" direction="in"/>
      <arg type="u" name="param2" direction="in"/>
      <arg type="u" name="param3" direction="in"/>
    </signal>


  </interface>
</node>
