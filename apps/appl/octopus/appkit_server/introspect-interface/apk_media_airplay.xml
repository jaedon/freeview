<?xml version="1.0" ?>
<!-- ref. http://dbus.freedesktop.org/doc/dbus-specification.html -->
<node name="/Octopus/Appkit/Media/Airplay">   
    <interface name="Octopus.Appkit.Media.Airplay">

    <method name="Start">
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>
  
    <method name="Stop">
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>

    <method name="SetPlaybackInfo">
    	<arg type="(uxxxxxi)" name="playbackInfo" direction="in" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>

    <signal name="event">
      <arg type="u" name="msgInt" direction="in"/>
      <arg type="s" name="msgStr" direction="in"/>
      <arg type="u" name="param1" direction="in"/>
      <arg type="u" name="param2" direction="in"/>
    </signal>


  </interface>
</node>
