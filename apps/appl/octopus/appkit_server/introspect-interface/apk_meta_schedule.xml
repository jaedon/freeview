<?xml version="1.0" ?>
<!-- ref. http://dbus.freedesktop.org/doc/dbus-specification.html -->
<node name="/Octopus/Appkit/Meta/Schedule">   
    <interface name="Octopus.Appkit.Meta.Schedule">

    <method name="GetSchedule">
    	<arg type="u" name="slot" direction="in" />
    	<arg type="((uuu(uuu)uuuuuubu)ub(uu)aybb)" name="schedule" direction="out" />
