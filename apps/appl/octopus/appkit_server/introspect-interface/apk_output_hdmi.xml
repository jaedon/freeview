<?xml version="1.0" ?>
<!-- ref. http://dbus.freedesktop.org/doc/dbus-specification.html -->
<node name="/Octopus/Appkit/Output/Hdmi">   
    <interface name="Octopus.Appkit.Output.Hdmi">

    <method name="SupportCec">
    	<arg type="b" name="support" direction="out" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>
  
    <method name="GetHdcpStatus">
    	<arg type="u" name="hdcpStatus" direction="out" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>

    <method name="Set3DSelect">
    	<arg type="u" name="hdmi3DSelect" direction="in" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>
    
    <method name="Get3DSelect">
    	<arg type="u" name="hdmi3DSelect" direction="out" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>    

    <method name="GetEdid3DFormatCapability">
    	<arg type="u" name="edid3DFormatCapability" direction="out" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>       

    <method name="Get3DStatus">
    	<arg type="u" name="hdmi3DStatus" direction="out" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>  

    <method name="GetEdidResolutionCapability">
    	<arg type="u" name="resolutionCapability" direction="out" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>  

    <method name="SetCecEnable">
    	<arg type="b" name="support" direction="in" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>
                                                                            
    <method name="GetCecEnable">
    	<arg type="b" name="support" direction="out" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>
    
    
    <signal name="event">
    </signal>


  </interface>
</node>
