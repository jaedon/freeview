<?xml version="1.0" ?>
<!-- ref. http://dbus.freedesktop.org/doc/dbus-specification.html -->
<node name="/Octopus/Appkit/Util/Datastreamer">   
    <interface name="Octopus.Appkit.Util.Datastreamer">

    <method name="ConnectDataStreamer">
    	<arg type="s" name="dataType" direction="in" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>
  
    <method name="DisconnectDataStreamer">
    	<arg type="s" name="dataType" direction="in" />
      	<annotation name="org.freedesktop.DBus.Method.NoReply" value="error"/>
    </method>
                                                                        

    <signal name="event">
      <arg type="ay" name="data" direction="in"/>
    </signal>


  </interface>
</node>
