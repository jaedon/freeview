lxc.utsname = corsair
lxc.tty = 1
lxc.pts = 1024
lxc.rootfs = /home/corsair/root
lxc.mount = /home/corsair/fstab
lxc.mount.entry=/lib /home/corsair/root/lib none ro,bind 0 0
lxc.mount.entry=/usr/lib /home/corsair/root/usr/lib none ro,bind 0 0

lxc.network.type = veth
lxc.network.flags = up
lxc.network.link = br0
lxc.network.name = eth0
lxc.network.hwaddr = 00:01:02:03:00:12
lxc.network.ipv4 = 10.255.128.212/24
