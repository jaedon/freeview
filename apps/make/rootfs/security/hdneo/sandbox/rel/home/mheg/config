lxc.utsname = mheg
lxc.tty = 1
lxc.pts = 1024
lxc.rootfs = /home/mheg/root
lxc.mount = /home/mheg/fstab
lxc.mount.entry=/lib /home/mheg/root/lib none ro,bind 0 0
lxc.mount.entry=/usr/lib /home/mheg/root/usr/lib none ro,bind 0 0

lxc.network.type = veth
lxc.network.flags = up
lxc.network.link = br0
lxc.network.name = eth0
lxc.network.hwaddr = 00:01:02:03:00:15
lxc.network.ipv4 = 10.255.128.215/24


