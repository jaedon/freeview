ifeq ($(CONFIG_LXC),y)
	@echo -- LXC
# mkdir to mount cgroup
	@mkdir -p $(HUMAX_PREPARE_ROOT_DIR)/cgroup
# copy libs to rootfs
	@$(HUMAX_CP) -drpf $(HUMAX_PACKAGE_DIR)/lxc/* $(HUMAX_PREPARE_ROOT_DIR)/
	@$(HUMAX_CP) -drpf $(HUMAX_PACKAGE_DIR)/libcap/sbin/* $(HUMAX_PREPARE_ROOT_DIR)/usr/sbin/
	@$(HUMAX_CP) -drpf $(HUMAX_PACKAGE_DIR)/libpam/lib/libpam.so* $(HUMAX_PREPARE_ROOT_DIR)/usr/lib/
	@$(HUMAX_CP) -drpf $(HUMAX_UCLIBC_OLD_DIR)/sys-root/lib/libutil* \
						$(HUMAX_PREPARE_ROOT_DIR)/lib/
# copy smcroute to rootfs
	@$(HUMAX_CP) -drpf $(HUMAX_PACKAGE_DIR)/smcroute/* \
						$(HUMAX_PREPARE_ROOT_DIR)/
# copy mdns repeater to rootfs
	@$(HUMAX_CP) -drpf $(HUMAX_PACKAGE_DIR)/mdns-repeater/* \
						$(HUMAX_PREPARE_ROOT_DIR)/

# copy libcap/libattr to rootfs
	@$(HUMAX_CP) -drpf $(HUMAX_PACKAGE_DIR)/libcap/lib64/libcap.so* $(HUMAX_PREPARE_ROOT_DIR)/usr/lib/
	@$(HUMAX_CP) -drpf $(HUMAX_PACKAGE_DIR)/libattr/lib/libattr.so* $(HUMAX_PREPARE_ROOT_DIR)/usr/lib/
endif

