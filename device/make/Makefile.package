	@$(HUMAX_CP) -rf $(HUMAX_PACKAGE_DIR)/mtd-utils/usr/sbin/ubi* $(HUMAX_PREPARE_ROOT_DIR)/usr/sbin/
	@$(HUMAX_CP) -rf $(HUMAX_PACKAGE_DIR)/mtd-utils/usr/sbin/flash_erase $(HUMAX_PREPARE_ROOT_DIR)/usr/sbin/
	@$(HUMAX_CP) -rf $(HUMAX_PACKAGE_DIR)/mtd-utils/usr/sbin/mtdinfo $(HUMAX_PREPARE_ROOT_DIR)/usr/sbin/
endif
endif	
