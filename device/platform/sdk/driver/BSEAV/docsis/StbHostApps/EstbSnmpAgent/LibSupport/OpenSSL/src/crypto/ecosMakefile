         #$(X509V3_DIR)/tabtest.c


#//======================================================================
#// Source Files
#//======================================================================
SOURCE_C=$(CRYPTO_C)  $(AES_C)    $(ASN1_C)   $(BIO_C)    $(BN_C)      $(BUFFER_C) \
         $(COMP_C)    $(CONF_C)   $(DES_C)    $(DH_C)     $(ERR_C)\
	 $(EVP_C)     $(HMAC_C)   $(LHASH_C)  $(MD5_C)    $(OBJECTS_C) $(PEM_C) \
	 $(RAND_C)    $(RSA_C)    $(SHA_C)    $(STACK_C)  $(UI_C)      $(X509_C) $(X509V3_C)     

#OpenSSH needs these
SOURCE_C+= $(RC4_C) $(DSA_C) $(PKCS12_C)
            
         # don't use the following: $(IDEA_C) $(MDC2_C) $(RC5_C) $(BF_C)
	 
	 # $(RC2_C)    $(MD2_C)   $(MD4_C)    $(RIPEMD_C)  $(KRB5_C)   
	 # $(TXT_DB_C) $(STORE_C) $(PQUEUE_C) $(THREADS_C) $(OCSP_C)   $(ENGINE_C) 
	 # $(ECDH_C)   $(EC_C)    $(ECDSA_C)  $(DSO_C)     $(PKCS7_C)  
	 # $(CAST_C)       
	 


#To Add Blowfish 
#SOURCE_C+=$(BF_C) 


#//======================================================================
#// Generic rule for generating dependencies files
#//======================================================================
%.d : %.c
	$(COMPILE_DEPENDS) -M $< > $(@F)
   
#//======================================================================
#// Generic rule for generating *.o object files
#//======================================================================
%.o: %.c
	$(COMPILE) $< -o $@
   
#//======================================================================
%.dir: 
	$(MAKE) -C $* 

#//======================================================================
%.cln: 
	$(MAKE) -C $* clean
   
#//======================================================================
#// Internal definitions
#//======================================================================
allcfiles      = $(SOURCE_C)
c_objects      = $(allcfiles:.c=.o)

all_objects   := $(c_objects) 

#//======================================================================
#// Build targets
#//======================================================================
target : project

project : $(all_objects) $(PROJECTLIB)

$(PROJECTLIB): $(all_objects)
	$(ARCHIVE) $?

all : 
	@$(MAKE) clean
	@$(MAKE) target

clean : $(c_subdirs)
	rm -f *.o
	rm -f aes/*.o
	rm -f asn1/*.o
	rm -f bf/*.o
	rm -f bio/*.o
	rm -f bn/*.o
	rm -f buffer/*.o
	rm -f cast/*.o
	rm -f comp/*.o
	rm -f conf/*.o
	rm -f des/*.o
	rm -f dh/*.o
	rm -f dsa/*.o
	rm -f dso/*.o
	rm -f ec/*.o
	rm -f ecdh/*.o
	rm -f ecdsa/*.o
	rm -f engine/*.o
	rm -f err/*.o
	rm -f evp/*.o
	rm -f hmac/*.o
	rm -f idea/*.o
	rm -f krb5/*.o
	rm -f lhash/*.o
	rm -f md2/*.o
	rm -f md4/*.o
	rm -f md5/*.o
	rm -f mdc2/*.o
	rm -f objects/*.o
	rm -f ocsp/*.o
	rm -f pem/*.o
	rm -f pkcs7/*.o
	rm -f pkcs12/*.o
	rm -f pqueue/*.o
	rm -f rand/*.o
	rm -f rc2/*.o
	rm -f rc4/*.o
	rm -f rc5/*.o
	rm -f ripemd/*.o
	rm -f rsa/*.o
	rm -f sha/*.o
	rm -f stack/*.o
	rm -f store/*.o
	rm -f threads/*.o
	rm -f txt_db/*.o
	rm -f ui/*.o
	rm -f x509/*.o
	rm -f x509v3/*.o
	rm -f $(PROJECTLIB)



