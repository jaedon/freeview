      $(SHA_DIR)/sha256t.c  $(SHA_DIR)/sha512.c   $(SHA_DIR)/sha512t.c  $(SHA_DIR)/sha.c\
      $(SHA_DIR)/sha_dgst.c $(SHA_DIR)/sha_one.c

      #$(SHA_DIR)/sha1test.c  $(SHA_DIR)/shatest.c


#//======================================================================
#// STACK Files
