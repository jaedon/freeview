DRM_BUILD_ARCH=X86
DRM_BUILD_PLATFORM=ANSI
DRM_BUILD_PROFILE=OEM
DRM_BUILD_TYPE=CHECKED
DRM_TEST_PATH=$(shell pwd)
DRM_ROOT_PATH=$(subst /test,,$(DRM_TEST_PATH))
DRM_SOURCE_PATH=$(DRM_ROOT_PATH)/source

export DRM_SOURCE_PATH DRM_ROOT_PATH DRM_TEST_PATH

-include ./dirs
-include ./sources

include $(DRM_TEST_PATH)/default.mak
