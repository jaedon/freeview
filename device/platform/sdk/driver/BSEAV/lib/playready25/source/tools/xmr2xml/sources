SOURCES =     \
    xmr2xml.c \


INCLUDES =                                                \
    $(INCLUDES);                                          \
    $(_DRM_SOURCE_PATH)\tools\common;                     \
    $(_DRM_SOURCE_PATH)\tools\devicedevcert\bcertdump;    \
    $(_DRM_SOURCE_PATH)\tools\drmviewer;

